    function postSearcherForm() {
        var nameKeyword = jQuery("#nameKeyword").val();
        nameKeyword = jQuery.trim(nameKeyword);
        if (nameKeyword == "" || nameKeyword == null||nameKeyword == "Choose Job Category") {//搜索器不能为空
            alert("Choose Job Category！");
            jQuery("#nameKeyword").focus();
            return false;
        }
        var LocList = jQuery("#LocList").val();
        if(LocList=="Location")
        {
            jQuery("#LocList").val("");
        }
        var kw = jQuery("#kw").val();
        if(kw=="Keyword(s)")
        {
           jQuery("#kw").val("");
        }
        var form = jQuery("form#searchForm");
        form.attr("action","./jobs/keyword_search.aspx");
        form.attr("method","GET");
        form.submit();
        return true;
    }
    
